<h1>ユーザーマイページ</h1>
<% if @reviews.any? %>
  <% @reviews.each do |review| %>
    <ul>
      <li><%= review.title %></li>
      <li><%= link_to '編集', edit_hackathon_review_path(review.hackathon_id, review.id), class: 'btn btn-success' %></li>
    </ul>
  <% end %>
<% end %>

